{
  "name": "@effect/schema",
  "version": "0.73.4",
  "description": "Modeling the schema of data structures as first-class values",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Effect-TS/effect.git",
    "directory": "packages/schema"
  },
  "sideEffects": [],
  "dependencies": {
    "fast-check": "^3.21.0"
  },
  "peerDependencies": {
    "effect": "^3.8.3"
  },
  "publishConfig": {
    "provenance": true
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/dts/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/dts/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./AST": {
      "types": "./dist/dts/AST.d.ts",
      "import": "./dist/esm/AST.js",
      "default": "./dist/cjs/AST.js"
    },
    "./Arbitrary": {
      "types": "./dist/dts/Arbitrary.d.ts",
      "import": "./dist/esm/Arbitrary.js",
      "default": "./dist/cjs/Arbitrary.js"
    },
    "./ArrayFormatter": {
      "types": "./dist/dts/ArrayFormatter.d.ts",
      "import": "./dist/esm/ArrayFormatter.js",
      "default": "./dist/cjs/ArrayFormatter.js"
    },
    "./Equivalence": {
      "types": "./dist/dts/Equivalence.d.ts",
      "import": "./dist/esm/Equivalence.js",
      "default": "./dist/cjs/Equivalence.js"
    },
    "./FastCheck": {
      "types": "./dist/dts/FastCheck.d.ts",
      "import": "./dist/esm/FastCheck.js",
      "default": "./dist/cjs/FastCheck.js"
    },
    "./JSONSchema": {
      "types": "./dist/dts/JSONSchema.d.ts",
      "import": "./dist/esm/JSONSchema.js",
      "default": "./dist/cjs/JSONSchema.js"
    },
    "./ParseResult": {
      "types": "./dist/dts/ParseResult.d.ts",
      "import": "./dist/esm/ParseResult.js",
      "default": "./dist/cjs/ParseResult.js"
    },
    "./Pretty": {
      "types": "./dist/dts/Pretty.d.ts",
      "import": "./dist/esm/Pretty.js",
      "default": "./dist/cjs/Pretty.js"
    },
    "./Schema": {
      "types": "./dist/dts/Schema.d.ts",
      "import": "./dist/esm/Schema.js",
      "default": "./dist/cjs/Schema.js"
    },
    "./Serializable": {
      "types": "./dist/dts/Serializable.d.ts",
      "import": "./dist/esm/Serializable.js",
      "default": "./dist/cjs/Serializable.js"
    },
    "./TreeFormatter": {
      "types": "./dist/dts/TreeFormatter.d.ts",
      "import": "./dist/esm/TreeFormatter.js",
      "default": "./dist/cjs/TreeFormatter.js"
    }
  },
  "typesVersions": {
    "*": {
      "AST": [
        "./dist/dts/AST.d.ts"
      ],
      "Arbitrary": [
        "./dist/dts/Arbitrary.d.ts"
      ],
      "ArrayFormatter": [
        "./dist/dts/ArrayFormatter.d.ts"
      ],
      "Equivalence": [
        "./dist/dts/Equivalence.d.ts"
      ],
      "FastCheck": [
        "./dist/dts/FastCheck.d.ts"
      ],
      "JSONSchema": [
        "./dist/dts/JSONSchema.d.ts"
      ],
      "ParseResult": [
        "./dist/dts/ParseResult.d.ts"
      ],
      "Pretty": [
        "./dist/dts/Pretty.d.ts"
      ],
      "Schema": [
        "./dist/dts/Schema.d.ts"
      ],
      "Serializable": [
        "./dist/dts/Serializable.d.ts"
      ],
      "TreeFormatter": [
        "./dist/dts/TreeFormatter.d.ts"
      ]
    }
  }
}