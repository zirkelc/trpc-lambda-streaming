#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/find-requires@1.0.0/node_modules/find-requires/bin/node_modules:/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/find-requires@1.0.0/node_modules/find-requires/node_modules:/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/find-requires@1.0.0/node_modules:/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/find-requires@1.0.0/node_modules/find-requires/bin/node_modules:/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/find-requires@1.0.0/node_modules/find-requires/node_modules:/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/find-requires@1.0.0/node_modules:/Users/zirkelc/Code/trpc-lambda-streaming/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../find-requires/bin/find-requires.js" "$@"
else
  exec node  "$basedir/../find-requires/bin/find-requires.js" "$@"
fi
